var
    int: i, j;
    bool: b;
    string: s;
begin
    i := 4;
    b := true;
    s := "teste";
    write i + j;
    write i*2;
    write b and (i != 5);
    write s;
    if i > 2
        write i/2;
    else
        write i;
    end
    while i <= 10
        write i;
        i := i + 1;
    end
    
end
